package prodPlan;

/**
 * Class to abstract Caracteristicas
 * @author ra146446
 *
 */
public class Caracteristica {
	String nome;
	String conteudo;
	
	/**
	 * 
	 * @param name
	 * @param content
	 */
	public Caracteristica(String name, String content) {
		this.nome = name;
		this.conteudo = content;
	}

}
